<%= form_for current_user, url: submit_info_path, method: :post do |f| %>

  <%= f.fields_for :myrsc do |m| %>
    <%= m.label :username, 'MyRsc username' %>
    <%= m.text_field :username %>
    <br/>
    <%= m.label :password, 'MyRsc password' %>
    <%= m.password_field :password %>
  <% end %>
  <br/>
  <br/>
  <%= f.fields_for :oxfords do |o| %>
    <%= o.label :username, 'Oxford username' %>
    <%= o.text_field :username %>
    <br/>
    <%= o.label :password, 'Oxford password' %>
    <%= o.password_field :password %>
    <br/>
    <%= o.label :claimant, 'Oxford claimant' %>
    <%= o.text_field :claimant %>
    <br/>
    <%= o.label :relationship, 'Oxford relationship' %>
    <%= o.text_field :relationship %>
    <br/><br/>
  <% end %>
  <%= submit_tag %>
<% end %>
